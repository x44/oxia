function Component() {
	return (
		<div>
			<span>Component</span>
			<slot></slot>
			<div ifSlotEmpty>
				DIV
			</div>
		</div>
	)

	<style slot>
		span {
			color: orange;
		}
	</style>
}

function Child() {
	return <span>Child</span>
}

export default function index() {
	return (
		<Component><span>Hello</span></Component>
		<Component><Child></Child></Component>
	)
}